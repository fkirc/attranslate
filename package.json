{
  "name": "attranslate",
  "version": "2.1.0",
  "description": "Semi-automated Text Translator for Websites and Apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fkirc/attranslate.git"
  },
  "keywords": [
    "semi-automated translation",
    "openai-translate",
    "google-translate",
    "website translation",
    "mobile app translation",
    "i18n"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fkirc/attranslate/issues"
  },
  "homepage": "https://github.com/fkirc/attranslate#readme",
  "bin": {
    "attranslate": "bin/attranslate"
  },
  "main": "./dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm link",
    "clean": "rm -rf ./dist",
    "lint": "npm run eslint",
    "reformat": "npm run eslint -- --fix",
    "eslint": "./node_modules/.bin/eslint '**/*.{ts,tsx}' --ignore-path=.gitignore",
    "test": "jest --testTimeout=10000",
    "test:windows": "npm run test -- --config=test/windows.jest.config.js",
    "test:generate_refs": "GENERATE_REFS=True npm run test",
    "prepublishOnly": "git diff --exit-code"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.0.3",
    "@types/semver": "^7.5.0",
    "axios": "^1.4.0",
    "clipboardy": "^2.3.0",
    "commander": "^6.1.0",
    "detect-indent": "^6.0.0",
    "dotenv": "^16.3.1",
    "gettext-parser": "^4.0.4",
    "inquirer": "^7.3.3",
    "messageformat-parser": "^4.1.0",
    "openai": "^3.2.0",
    "semver": "^7.5.4",
    "typechat": "^0.0.10",
    "xml2js": "https://github.com/fkirc/node-xml2js/tarball/680ee7ed2dad8b9ddacc37b5e4b4a671706e0bcf",
    "yaml": "2.0.0-1"
  },
  "devDependencies": {
    "@types/gettext-parser": "^4.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.11.8",
    "@types/node-fetch": "^2.1.6",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.2",
    "lint-staged": "^10.4.0",
    "node-fetch": "^2.3.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "node_modules/.bin/eslint --ignore-path=.gitignore --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "node_modules/.bin/lint-staged"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "test"
    ]
  }
}
